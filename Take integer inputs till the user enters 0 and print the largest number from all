import java.util.Scanner;

public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int largest_No = Integer.MIN_VALUE;
        while(true){
            int i = sc.nextInt();
            if(i==0){
                System.out.println(largest_No);
                break;
            }else if(largest_No<i){
                largest_No=i;
            }

        }
    }
}
