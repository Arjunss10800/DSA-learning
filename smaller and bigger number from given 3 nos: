import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        System.out.println(smaller(a, b,c));
        System.out.println(bigger(a, b, c));
    }
    static int smaller(int a, int b,  int c){
        int small = a;
        if(small>b){
            small = b;
        }
        if(small>c){
            small = c;
        }
        return small;

    }
    static int bigger(int a, int b,  int c){
        int big = a;
        if(big <b){
            big = b;
        }
        if(big<c){
            big = c;
        }
        return big;
    }
}
