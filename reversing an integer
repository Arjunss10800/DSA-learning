import java.util.Scanner;
// reversing of an integer
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int output = 0;
        while(a>0){
            int lastTerm = a%10;
            output = (output*10)+lastTerm;
            a = a/10;
        }
        System.out.println(output);
    }
}
